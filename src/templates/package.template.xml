<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<umbPackage>
  <info>
    <package>
      <name><%= name %></name>
      <version><%= version %></version>
      <license url="<%= licenseUrl %>"><%= license %></license>
      <url><%= url %></url>
      <requirements>
        <major>8</major>
        <minor>1</minor>
        <patch>0</patch>
      </requirements>
    </package>
    <author>
      <name><%= author %></name>
      <website><%= authorUrl %></website>
    </author>
    <readme><![CDATA[<%= readme %>]]></readme>
  </info>
  <DocumentTypes />
  <Templates />
  <Stylesheets />
  <Macros />
  <DictionaryItems />
  <Languages />
  <DataTypes />
  <control />
  <Action />
  <files>
    <% files.forEach(function(file) { %>
    <file>
      <guid><%= file.guid %></guid>
      <orgPath><%= file.dir %></orgPath>
      <orgName><%= file.name %></orgName>
    </file>
    <% }); %>
  </files>
</umbPackage>